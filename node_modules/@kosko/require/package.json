  },
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@kosko/common-utils": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "execa": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "@kosko/build-scripts": "^0.0.0",
    "@kosko/jest-preset": "^0.0.0",
    "@kosko/test-utils": "^0.0.0"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  },
  "scripts": {
    "build": "build-scripts"
  }
