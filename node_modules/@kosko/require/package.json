      "types": "./dist/types.d.ts",
      "require": "./dist/index.node.cjs"
    },
    "import": {
      "types": "./dist/types.d.mts",
      "default": "./dist/index.base.mjs"
    },
    "types": "./dist/types.d.ts",
    "require": "./dist/index.node.cjs"
  },
  "files": [
    "/dist/",
    "/lib/"
  ],
  "keywords": [
    "kosko"
  ],
